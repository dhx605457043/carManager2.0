<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.car.manager.dao.CargoListMapper">
<select id="selectAllCargo" resultType="com.car.manager.entity.CargoList">
    select
        cargo_id as cargoId,
        cargo_name as cargoName
    from cargo_list
</select>
</mapper>